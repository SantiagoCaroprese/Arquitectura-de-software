<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>/inicio.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>iniciarSesion</from-outcome>
			<to-view-id>/iniciarSesion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{controladorObtenerProductosCatalogo.verCatalogo}</from-action>
			<from-outcome>true</from-outcome>
			<to-view-id>/verCatalogo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{controladorObtenerProductosCatalogo.verCatalogo}</from-action>
			<from-outcome>false</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pedidoDomicilio</from-outcome>
			<to-view-id>/iniciarPedidoDomicilio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pedidoRecoger</from-outcome>
			<to-view-id>/iniciarPedidoRecoger.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/verCatalogo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/iniciarPedidoDomicilio.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pedidoRecoger</from-outcome>
			<to-view-id>/iniciarPedidoRecoger.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/iniciarPedidoRecoger.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/verCarrito.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/verCatalogoFranquicia.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>catalogoFranquicia</from-outcome>
			<to-view-id>/verCatalogoFranquicia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
